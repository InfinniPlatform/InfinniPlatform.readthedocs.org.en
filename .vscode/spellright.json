//
// Spell Right workspace configuration file.
//
{
    "language": "en",
    "documentTypes": [
        "markdown",
        "latex",
        "plaintext",
        "restructuredtext"
    ],
    "groupDictionaries": true,
    "statusBarIndicator": true,
    "suggestionsInHints": true,
    "ignoreWords": [
        "deserialize",
        "Json",
        "InfinniPlatfrom",
        "Serializer",
        "serializers",
        "deserializes",
        "deserialized",
        "Infinni",
        "deserializing",
        "nginx",
        "Cron",
        "multitenancy"
    ],
    "ignoreRegExps": [
        "/\\s{4,}.*/g"
    ],
    "ignoreFiles": [
        "**/.gitignore",
        "**/.spellignore"
    ]
}