{
    "metadata": [
        {
            "src": [
                {
                    "files": [
                        "**.csproj"
                    ],
                    "exclude": [
                        "**/obj/**",
                        "**/bin/**",
                        "_site/**",
                        ".git/**",
                        ".vs/**",
                        "Assemblies/**",
                        "Files/**",
                        "*.Tests/**"
                    ],
                    "src": "../../../InfinniPlatform"
                }
            ],
            "dest": "reference"
        }
    ],
    "build": {
        "content": [
            {
                "files": [
                    "reference/**.yml",
                    "reference/index.md"
                ]
            },
            {
                "files": [
                    "toc.yml",
                    "*.md"
                ],
                "exclude": [
                    "obj/**",
                    "_site/**"
                ]
            }
        ],
        "dest": "../source/_docfx/api",
        "globalMetadataFiles": [],
        "globalMetadata": {
            "_appFooter": "© Copyright Infinnity Solutions Ltd, 2010–2017.",
            "_appLogoPath": "../../_static/favicon.png",
            "_appFaviconPath": "../../_static/favicon.png"
        },
        "fileMetadataFiles": [],
        "template": [
            "default"
        ],
        "postProcessors": [],
        "noLangKeyword": false
    }
}